{
    "id": "boxlang",
    "version": "1.0.2",
    "name": "BoxLang",
    "description": "CommandBox based BoxLang development environment.",
    "documentationURL": "https://github.com//jbeers/devcontainer-template-boxlang/tree/main",
    "publisher": "Ortus Solutions",
    "licenseURL": "https://github.com/devcontainers/templates/blob/main/LICENSE",
    "options": {
        "engine": {
            "type": "string",
            "enum": [
                "BoxLang",
                "Lucee", 
                "Adobe"
            ],
            "default": "BoxLang",
            "description": "Choose the CFML engine to use for development"
        }
    },
    "platforms": ["Any"],
    "optionalPaths": [
        ".github/*"
    ]
}