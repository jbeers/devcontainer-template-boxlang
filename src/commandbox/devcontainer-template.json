{
    "id": "commandbox",
    "version": "1.0.9",
    "name": "CommandBox",
    "description": "CommandBox based development environment.",
    "documentationURL": "https://github.com//jbeers/devcontainer-template-boxlang/tree/main",
    "publisher": "Ortus Solutions",
    "licenseURL": "https://github.com/devcontainers/templates/blob/main/LICENSE",
    "options": {
        "engine": {
            "type": "string",
            "proposals": [
                "boxlang",
                "boxlang-snapshot",
                "lucee5", 
                "lucee6",
                "adobe2023",
                "adobe2021"
            ],
            "default": "boxlang",
            "description": "Choose the engine to use for development"
        }
    },
    "platforms": ["Any"],
    "optionalPaths": [
        ".github/*",
        "index.bxm",
        "index.cfm"
    ]
}